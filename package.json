    "remark-toc": "^5.1.1",
    "remark-unlink": "^2.1.1",
    "sass-loader": "^7.1.0",
    "sharp": "^0.22.1",
    "slugify": "^1.3.4",
    "strip-markdown": "^3.0.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.3",
    "unified": "^7.1.0",
