{
  "name": "andretorgal.com",
  "private": true,
  "scripts": {
    "dev": "node -r ts-node/register node_modules/.bin/react-static start",
    "serve-statics": "serve statics/ -p 3333",
    "start": "concurrently --kill-others 'yarn serve-statics' 'yarn dev'",
    "clean": "find . -name '*.scss.d.ts' -type f -delete && rm -rf artifacts/ dist/ tmp/",
    "lint": "yarn lint-style && yarn lint-es && yarn tsc",
    "lint-style": "stylelint 'src/**/*.scss' --syntax scss --cache --cache-location ./tmp/stylelint.cache",
    "lint-es": "eslint --ext .js,.ts,.tsx src/ .react-static/ .webpack/ --cache --cache-location ./tmp/eslint.cache",
    "tsc": "tsc --noEmit",
    "lint-fix": "yarn lint-style --fix && yarn lint-es --fix && yarn tsc",
    "build:stage": "export BUILD_STAGE=stage && node -r ts-node/register node_modules/.bin/react-static build --staging",
    "build:prod": "export BUILD_STAGE=prod && node -r ts-node/register node_modules/.bin/react-static build",
    "analyze": "export BUILD_STAGE=prod && node -r ts-node/register node_modules/.bin/react-static build --analyze",
    "serve": "serve dist -p 3000",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "chokidar": "^2.1.6",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "dayjs": "^1.8.14",
    "fs-extra-promise": "^1.0.1",
    "gray-matter": "^4.0.2",
    "intersection-observer": "^0.7.0",
    "mdast-util-to-string": "^1.0.5",
    "node-sass": "^4.12.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-scss": "^2.0.0",
    "postcss-utilities": "^0.8.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-intersection-observer": "^8.23.0",
    "react-router-dom": "^5.0.0",
    "react-static": "7.2.2",
    "react-static-plugin-react-router": "^7.2.2",
    "react-static-plugin-sitemap": "^7.2.2",
    "react-static-plugin-source-filesystem": "^7.2.2",
    "react-static-plugin-typescript": "^7.2.2",
    "rehype-autolink-headings": "^2.0.4",
    "rehype-highlight": "^2.2.1",
    "rehype-parse": "^6.0.0",
    "rehype-raw": "^4.0.0",
